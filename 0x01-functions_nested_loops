#include <stdio.h>
#include"main"
/**
 * main - using putchar to the standard io
 * printing putchar
 * Return;0
 * /
 int main(void)
 {
        char *x = "_putchar";
        while (*x)
        {
               _putchar(*x);
               x++;
        }
        _putchar('\n');
    return (0);
    }


